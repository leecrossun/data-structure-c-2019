      <p align="center"><img src="images/SortApplet_html_smartbutton1.gif" namo_npi=":EmbededNPI1" border="0">
<p><applet
  codebase = "./"
  code     = "SortApplet.class"
  name     = "TestApplet"
  width    = "700"
  height   = "396"
  hspace   = "0"
